{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Study Day",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "title": {
      "type": "integer"
    },
    "words": {
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "./word.json"
      }
    }
  },
  "required": ["title", "words"]
}
